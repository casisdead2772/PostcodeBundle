{
    "name": "casisdead2772/postcode-bundle",
    "version": "0.1.0",
    "type": "symfony-bundle",
    "description": "Symfony bundle getting address info with postcode by API services",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/framework-bundle": "6.0.*",
        "symfony/http-client": "^6.0",
        "symfony/runtime": "6.0.*",
        "symfony/yaml": "6.0.*"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "Casisdead2772\\PostcodeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Casisdead2772\\PostcodeBundle\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    }
}
